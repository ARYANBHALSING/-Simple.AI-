l = float(input("Enter the length of the rectangle = "))
b = float(input("Enter the Breadth of the rectangle = "))
perimeter = 2*(l+b)
area = l*b
print("The perimeter of rectangle having length ",l,"and breadth ",b," is = ",perimeter)
print("The area of rectangle having length ",l,"and breadth ",b," is = ",area)
